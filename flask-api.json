{
  "name": "flask-api",
  "api_id": "1",
  "org_id": "default",
  "use_keyless": true,
  "definition": {
    "location": "header",
    "key": "x-api-version"
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "expires": "",
        "paths": {
          "ignored": [],
          "white_list": [],
          "black_list": []
        },
        "extended_paths": {},
        "global_headers": {},
        "global_headers_remove": [],
        "global_size_limit": 0,
        "use_extended_paths": true
      }
    }
  },
  "proxy": {
    "listen_path": "/flask/",
    "target_url": "http://flask_app:5000/",
    "strip_listen_path": true
  },
  "active": true
}
